<script>
function hasClass(ele,cls) {
     return ele.className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)'));
}

document.getElementById("nav-expander").onclick = function checkclass(){


var crus = document.getElementById("crus");
var menu = document.getElementById("menu");
var expander = document.getElementById("nav-expander");

if(hasClass(document.getElementsByTagName("body")[0], "nav-expanded")){

menu.style.display = "initial";
crus.style.display = "none";
expander.style.height = "auto";
document.getElementById("productosinitial").style.display = "block";
document.getElementById("productoshide").style.display = "none";

document.getElementById("contact").style.display = "block";
document.getElementById("blog").style.display = "block";
document.getElementById("cart").style.display = "block";

}else{
menu.style.display = "none";
crus.style.display = "initial";
expander.style.height = "100vh";



}
}

</script>
<script>

function productoshide(){



if(hasClass(document.getElementById("botonproductos"), "open")){

document.getElementById("contact").style.display = "block";
document.getElementById("blog").style.display = "block";
document.getElementById("cart").style.display = "block";
document.getElementById("productosinitial").style.display = "block";
document.getElementById("productoshide").style.display = "none";

}else{



document.getElementById("contact").style.display = "none";
document.getElementById("blog").style.display = "none";
document.getElementById("cart").style.display = "none";

document.getElementById("productosinitial").style.display = "none";
document.getElementById("productoshide").style.display = "initial";

}




}

</script>

